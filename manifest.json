{
    "name":"So",
    "version":"1.0",
    "description":"extenci√≥n dar formato a enlaces de SO",
    "manifest_version": 2,
    "author":"Christian velasquez - Nakato and Arcanis",
    "background": {
        "persistent": false,
        "scripts": [
            "src/pages/js/background.js"
        ]
    },
    "browser_action": {
        "default_popup": "src/pages/html/popup.html",
        "default_title": "extencion de bloqueo de sitios web",
        "default_icon": {
            "32": "src/img/32.png",
            "48": "src/img/48.png",
            "128": "src/img/128.png"
        }
    },
    "icons": {
        "32": "src/img/32.png",
        "48": "src/img/48.png",
        "128": "src/img/128.png"
    },
    "content_scripts": [
        {
          "matches": ["https://*.com/*"],
          "js": ["src/pages/js/document.js"],
          "css": ["src/pages/css/style_links.css"],
          "run_at": "document_end"
        }
    ],
    "permissions":[
        "tabs",
        "http://*/*","https://*/*"
    ]
}